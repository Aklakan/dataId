@prefix datid: <https://raw.githubusercontent.com/dbpedia/dataId/master/ontology/dataid.ttl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix void: <http://rdfs.org/ns/void#> . 
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix sd: <http://www.w3.org/ns/sparql-service-description#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<>
    a owl:Ontology ;
    rdfs:comment "DBpedia Data ID is an ontology with the goal of describing LOD datasets via RDF files in a uniform way. Established vocabularies like DCAT, VoID, Prov-O and SPARQL Service Description are used for maximum compatibility."@en ;
    rdfs:label "DataID"@en .

datid:Dataset
    a rdfs:Class, owl:Class ;
    rdfs:subClassOf dcat:Dataset, void:Dataset, sd:Dataset, prov:Entity ;
    rdfs:label "Dataset"@en ;
	rdfs:subClassOf [
		a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty dct:title
	] , [
		a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty rdfs:label
	] , [
		a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty dct:description
	] , [
		a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty dct:issued
	] , [
		a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty dcat:contactPoint
	] , [
		a owl:Restriction ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty dct:publisher
	] , [
		a owl:Restriction ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty dct:creator
	] , [
		a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty void:rootResource
	] , [
		a owl:Restriction ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty void:exampleResource
	] , [
		a owl:Restriction ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty dct:language
	] , [
		a owl:Restriction ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty dcat:distribution
	] , [
		a owl:Restriction ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty dcat:landingPage
	] , [
		a owl:Restriction ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty prov:wasGeneratedBy
	] , [
		a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty dct:license
	] , [
		a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty dct:rights
	] .

datid:Distribution
    a rdfs:Class, owl:Class ;
    rdfs:subClassOf dcat:Distribution ;
    rdfs:comment "A way to access a dataset, like a dump file, an endpoint, an API etc."@en ;
    rdfs:label "Distribution"@en ;
    rdfs:subClassOf  [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty dct:title
    ] , [
		a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty dct:issued
	] , [
		a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty dct:license
	] , [
		a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty dct:rights
	] , [
		a owl:Restriction ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty dcat:accessURL
	] , [
		a owl:Restriction ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty dcat:mediaType
	] .

datid:Agent
	a rdfs:Class, owl:Class ;
	rdfs:subClassOf prov:Agent, foaf:Agent ;
	rdfs:label "Agent"@en ;
	rdfs:subClassOf  [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty rdfs:label
    ] , [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty foaf:name
    ] , [
        a owl:Restriction ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty foaf:mbox
    ] .
	
datid:Activity
	a rdfs:Class, owl:Class ;
	rdfs:subClassOf prov:Activity ;
	rdfs:label "Activity"@en ;
	rdfs:subClassOf  [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty dct:title
    ] , [
		a owl:Restriction ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty dct:publisher
	] .
	
datid:Entity
	a rdfs:Class, owl:Class ;
	rdfs:subClassOf prov:Entity ;
	rdfs:label "Activity"@en ;
	rdfs:subClassOf  [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty dct:title
    ] , [
		a owl:Restriction ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty dct:publisher
	] .

datid:Linkset
	a rdfs:Class, owl:Class ;
	rdfs:label "Linkset"@en ;
	rdfs:subClassOf  [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty void:subjectTarget
    ] , [
		a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty void:objectTarget
	] , [
		a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty void:triples
	] .
	
